{
  "manifest_version": 2,
  "name": "yomichan",
  "description": "description",
  "version": "1",
  "icons": {
    "64": "../icons/icon.png"
  },
  "background": {
    "scripts": ["./src/background_script_builded.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "run_at": "document_start",
      "js": ["./src/content_script_builded.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "64": "./icons/icon.png"
    },
    "default_title": "Test"
  },
  "page_action": {
    "default_icon": {
      "64": "./icons/icon.png"
    },
    "default_popup": "pageAction/index.html",
    "default_title": "Test"
  },
  "options_ui": {
    "page": "options/index.html"
  },
  "web_accessible_resources": [],
  "permissions": ["<all_urls>", "storage", "tabs", "activeTab"]
}
